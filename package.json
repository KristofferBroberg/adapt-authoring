{
  "name": "adapt-authoring",
  "version": "0.11.0",
  "description": "A server-based user interface for authoring eLearning courses using the Adapt Framework.",
  "homepage": "https://github.com/adapt-security/adapt-authoring",
  "license": "GPL-3.0",
  "bin": {
    "adaptat": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adapt-security/adapt-authoring.git"
  },
  "adapt_authoring": {
    "documentation": {
      "enable": true,
      "includes": {
        "docs": "docs/*"
      }
    },
    "framework": "5"
  },
  "dependencies": {
    "adapt-authoring-api": "git+https://github.com/adapt-security/adapt-authoring-api.git",
    "adapt-authoring-auth": "git+https://github.com/taylortom/adapt-authoring-auth.git",
    "adapt-authoring-config": "git+https://github.com/adapt-security/adapt-authoring-config.git",
    "adapt-authoring-core": "git+https://github.com/adapt-security/adapt-authoring-core.git",
    "adapt-authoring-lang": "git+https://github.com/adapt-security/adapt-authoring-lang.git",
    "adapt-authoring-logger": "git+https://github.com/adapt-security/adapt-authoring-logger.git",
    "adapt-authoring-mongodb": "git+https://github.com/adapt-security/adapt-authoring-mongodb.git",
    "adapt-authoring-server": "git+https://github.com/adapt-security/adapt-authoring-server.git"
  },
  "devDependencies": {
    "adapt-authoring-docs": "git+https://github.com/adapt-security/adapt-authoring-docs.git",
    "adapt-authoring-lint": "git+https://github.com/adapt-security/adapt-authoring-lint.git",
    "adapt-authoring-testing": "git+https://github.com/adapt-security/adapt-authoring-testing.git"
  },
  "engines": {
    "node": "12.x"
  }
}
