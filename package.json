{
  "name": "adapt-authoring",
  "version": "0.11.0",
  "description": "A server-based user interface for authoring eLearning courses using the Adapt Framework.",
  "homepage": "https://github.com/adapt-security/adapt-authoring",
  "license": "GPL-3.0",
  "bin": {
    "adaptat": "./bin/cli.js"
  },
  "repository": "github:adapt-security/adapt-authoring",
  "adapt_authoring": {
    "documentation": {
      "enable": true
    },
    "framework": "5"
  },
  "dependencies": {
    "adapt-authoring-api": "github:adapt-security/adapt-authoring-api.git",
    "adapt-authoring-auth": "github:taylortom/adapt-authoring-auth.git",
    "adapt-authoring-config": "github:adapt-security/adapt-authoring-config.git",
    "adapt-authoring-core": "github:adapt-security/adapt-authoring-core.git",
    "adapt-authoring-jsonschema": "github:adapt-security/adapt-authoring-jsonschema.git",
    "adapt-authoring-lang": "github:adapt-security/adapt-authoring-lang.git",
    "adapt-authoring-logger": "github:adapt-security/adapt-authoring-logger.git",
    "adapt-authoring-middleware": "github:adapt-security/adapt-authoring-middleware.git",
    "adapt-authoring-mongodb": "github:adapt-security/adapt-authoring-mongodb.git",
    "adapt-authoring-server": "github:adapt-security/adapt-authoring-server.git"
  },
  "devDependencies": {
    "adapt-authoring-docs": "github:adapt-security/adapt-authoring-docs",
    "adapt-authoring-lint": "github:adapt-security/adapt-authoring-lint",
    "adapt-authoring-testing": "github:adapt-security/adapt-authoring-testing"
  },
  "engines": {
    "node": "12.x"
  }
