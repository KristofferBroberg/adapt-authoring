{
  "name": "adapt-authoring",
  "version": "0.11.0",
  "description": "A server-based user interface for authoring eLearning courses using the Adapt Framework.",
  "homepage": "https://github.com/adapt-security/adapt-authoring",
  "license": "GPL-3.0",
  "repository": "adapt-security/adapt-authoring",
  "scripts": {
    "build": "cd node_modules/adapt-authoring-ui && node ../grunt/bin/grunt build:dev",
    "debug": "node --inspect --preserve-symlinks bin/cli start",
    "debug-brk": "node --inspect-brk --preserve-symlinks bin/cli start",
    "dev": "node --preserve-symlinks bin/cli start",
    "dev-init": "node bin/cli dev-init",
    "docs": "node --preserve-symlinks bin/cli docs",
    "generate-config": "node bin/cli generate-config",
    "lint": "node --preserve-symlinks bin/cli lint",
    "register-user": "node bin/cli register-user",
    "start": "node bin/cli start",
    "test": "node bin/cli test"
  },
  "dependencies": {
    "adapt-authoring-adaptFramework": "adapt-security/adapt-authoring-adaptFramework",
    "adapt-authoring-api": "adapt-security/adapt-authoring-api",
    "adapt-authoring-assets": "adapt-security/adapt-authoring-assets",
    "adapt-authoring-auth": "adapt-security/adapt-authoring-auth",
    "adapt-authoring-authored": "adapt-security/adapt-authoring-authored",
    "adapt-authoring-config": "adapt-security/adapt-authoring-config",
    "adapt-authoring-core": "adapt-security/adapt-authoring-core",
    "adapt-authoring-courseassets": "adapt-security/adapt-authoring-courseassets#events",
    "adapt-authoring-coursetheme": "adapt-security/adapt-authoring-coursetheme",
    "adapt-authoring-defaultplugins": "adapt-security/adapt-authoring-defaultplugins",
    "adapt-authoring-jsonschema": "adapt-security/adapt-authoring-jsonschema",
    "adapt-authoring-lang": "adapt-security/adapt-authoring-lang",
    "adapt-authoring-lang-en": "taylortom/adapt-authoring-lang-en",
    "adapt-authoring-localauth": "adapt-security/adapt-authoring-localauth",
    "adapt-authoring-logger": "adapt-security/adapt-authoring-logger",
    "adapt-authoring-mailer": "adapt-security/adapt-authoring-mailer",
    "adapt-authoring-middleware": "adapt-security/adapt-authoring-middleware",
    "adapt-authoring-mongodb": "adapt-security/adapt-authoring-mongodb",
    "adapt-authoring-mongodblogger": "adapt-security/adapt-authoring-mongodblogger",
    "adapt-authoring-roles": "adapt-security/adapt-authoring-roles",
    "adapt-authoring-server": "adapt-security/adapt-authoring-server",
    "adapt-authoring-sessions": "adapt-security/adapt-authoring-sessions",
    "adapt-authoring-spoortracking": "adapt-security/adapt-authoring-spoortracking",
    "adapt-authoring-tags": "adapt-security/adapt-authoring-tags",
    "adapt-authoring-ui": "adapt-security/adapt-authoring-ui",
    "adapt-authoring-usergroups": "adapt-security/adapt-authoring-usergroups",
    "adapt-authoring-users": "adapt-security/adapt-authoring-users"
  },
  "devDependencies": {
    "adapt-authoring-docs": "adapt-security/adapt-authoring-docs",
    "adapt-authoring-lint": "adapt-security/adapt-authoring-lint",
    "adapt-authoring-testing": "adapt-security/adapt-authoring-testing"
  },
  "engines": {
    "node": "14.x"
  }
}
